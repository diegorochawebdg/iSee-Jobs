package  {		import com.greensock.easing.Strong;	import com.greensock.TweenMax;	import flash.display.MovieClip;	import flash.events.Event;			public class Tela extends MovieClip {				public var maskMC:MovieClip;		public var asset:MovieClip;				public static const SPEED_TRANSITION:Number = 3;		public function Tela() {						maskMC = this["_maskMC"];			asset  = this["_asset"];						maskMC.cacheAsBitmap = 			asset.cacheAsBitmap  = true;						asset.mask = maskMC;						this.name = name;					}				public function init( animate:Boolean = true ):void 		{						if( animate ) 			{								TweenMax.to( maskMC , SPEED_TRANSITION, { y: 0, ease: Strong.easeOut, onComplete: complete });						} else {								maskMC.y = 0;				complete();			}		}				public function stopTela():void 		{					}						private function complete():void 		{			dispatchEvent( new Event(Event.COMPLETE));		}			}	}