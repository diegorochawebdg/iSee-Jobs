package {	import com.google.analytics.GATracker;	import flash.events.EventDispatcher;	import flash.events.IEventDispatcher;		public class GoogleAnalyticsTracker extends EventDispatcher	{		public static var ENABLED	:Boolean = true;		public static var GOOGLE_ANALYTICS_ID	:String = "UA-990606-1";		public static var DEBUG		:Boolean = true;		public static var TARGET	:String;						public function GoogleAnalyticsTracker(target:IEventDispatcher=null)		{						super(target);						}				/**		 * Função responsável por setar uma página no google analytics 		 * @param page		 * 		 */				public static function trackPage( page:String ):void 		{			if(ENABLED)			{				//trace("track: " + page );				var tracker:GATracker = new GATracker( CafeteiraMain._stage , GOOGLE_ANALYTICS_ID , "AS3", DEBUG );				tracker.trackPageview( page );			}		}			}}