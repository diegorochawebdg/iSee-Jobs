package  {	import flash.display.MovieClip;	import flash.events.MouseEvent;	import com.greensock.TweenMax;	import com.greensock.easing.*;		public class Tela2 extends Tela {				public var copo1:MovieClip;		public var copo2:MovieClip;		public var copo3:MovieClip;		public var copo4:MovieClip;				public function Tela2() {			// constructor code			super();						copo1 = asset.copo1;			copo2 = asset.copo2;			copo3 = asset.copo3;			copo4 = asset.copo4;						copo1.initY = copo1.y;			copo2.initY = copo2.y;			copo3.initY = copo3.y;			copo4.initY = copo4.y;						copo1.addEventListener(MouseEvent.MOUSE_OVER , sobeCopo );			copo1.addEventListener(MouseEvent.MOUSE_OUT  , desceCopo );			copo2.addEventListener(MouseEvent.MOUSE_OVER , sobeCopo );			copo2.addEventListener(MouseEvent.MOUSE_OUT  , desceCopo );			copo3.addEventListener(MouseEvent.MOUSE_OVER , sobeCopo );			copo3.addEventListener(MouseEvent.MOUSE_OUT  , desceCopo );			copo4.addEventListener(MouseEvent.MOUSE_OVER , sobeCopo );			copo4.addEventListener(MouseEvent.MOUSE_OUT  , desceCopo );								}				private function sobeCopo(e:MouseEvent):void 		{			var item:MovieClip = e.currentTarget as MovieClip;			TweenMax.to( item , 1 , { y: (item.initY - 20), ease: Back.easeOut });		}		private function desceCopo(e:MouseEvent):void 		{			var item:MovieClip = e.currentTarget as MovieClip;			TweenMax.to( item , 1 , { y: item.initY, ease: Back.easeOut });		}			}	}